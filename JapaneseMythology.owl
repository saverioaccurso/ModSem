     -->

    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Creatura_Mitologica -->

    <Class rdf:about="&japanesemythologyontology;Creatura_Mitologica">
        <rdfs:label xml:lang="it">CreaturaMitologica</rdfs:label>
        <rdfs:comment xml:lang="en">legendary creature or other mythological entity
</rdfs:comment>
        <rdfs:comment xml:lang="it"> creatura leggendaria o un&apos;altra entità mitologica</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Divinità -->

    <Class rdf:about="&japanesemythologyontology;Divinità">
        <rdfs:label xml:lang="it">Divinità</rdfs:label>
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="&japanesemythologyontology;èAssociataA"/>
                <onClass rdf:resource="&japanesemythologyontology;SacredPlace"/>
                <minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</minQualifiedCardinality>
            </Restriction>
        </equivalentClass>
        <rdfs:comment xml:lang="en">Represents the divine entity in Japanese mythology</rdfs:comment>
        <rdfs:comment xml:lang="it">Rappresenta l&apos;entità divina nella mitologia giapponese</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Epoca -->

    <Class rdf:about="&japanesemythologyontology;Epoca">
        <rdfs:label xml:lang="it">Epoca</rdfs:label>
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="&japanesemythologyontology;Ha_Evento"/>
                <onClass rdf:resource="&japanesemythologyontology;MythicalEvent"/>
                <minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</minQualifiedCardinality>
            </Restriction>
        </equivalentClass>
        <rdfs:comment xml:lang="en">It includes mythological cycles or specific eras in Japanese mythology</rdfs:comment>
        <rdfs:comment xml:lang="it">Comprende cicli mitologici o epoche specifiche nella mitologia giapponese</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#EventoAntico -->

    <Class rdf:about="&japanesemythologyontology;EventoAntico">
        <rdfs:label xml:lang="it">EventoAntico</rdfs:label>
        <rdfs:subClassOf rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:comment xml:lang="en">Event that occurred before the year 700</rdfs:comment>
        <rdfs:comment xml:lang="it">Evento accaduto prima dell&apos;anno 700</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#EventoDiUnificazione -->

    <Class rdf:about="&japanesemythologyontology;EventoDiUnificazione">
        <rdfs:label xml:lang="it">EventoDiUnificazione</rdfs:label>
        <rdfs:subClassOf rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:comment xml:lang="en">Event related to the unification of Japan</rdfs:comment>
        <rdfs:comment xml:lang="it">Evento legato all&apos;unificazione del giappone</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#EventoVendicativo -->

    <Class rdf:about="&japanesemythologyontology;EventoVendicativo">
        <rdfs:label xml:lang="it">EventoVendicativo</rdfs:label>
        <rdfs:subClassOf rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:comment xml:lang="en">Event where a Yurei is present as a mythological creature</rdfs:comment>
        <rdfs:comment xml:lang="it">Evento dove è presente uno Yurei come creatura mitologica</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Gioiello -->

    <Class rdf:about="&japanesemythologyontology;Gioiello">
        <rdfs:label xml:lang="it">Gioiello</rdfs:label>
        <rdfs:subClassOf rdf:resource="&japanesemythologyontology;SacredObject"/>
        <rdfs:comment xml:lang="en">Sacred jewel used in ceremonies and rites</rdfs:comment>
        <rdfs:comment xml:lang="it">Gioiello sacro usato in cerimonie e riti</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Hyosube -->

    <Class rdf:about="&japanesemythologyontology;Hyosube">
        <rdfs:label xml:lang="it">Hyosube</rdfs:label>
        <rdfs:subClassOf rdf:resource="&japanesemythologyontology;Kappa"/>
        <rdfs:comment xml:lang="en">Fur-covered Kappa variant</rdfs:comment>
        <rdfs:comment xml:lang="it">Variante di Kappa ricoperto di pelo</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#InsegnaImperiale -->

    <Class rdf:about="&japanesemythologyontology;InsegnaImperiale">
        <rdfs:label xml:lang="it">InsegnaImperiale</rdfs:label>
        <rdfs:subClassOf rdf:resource="&japanesemythologyontology;SacredObject"/>
        <rdfs:comment xml:lang="en">Sacred object that became a symbol of the empire due to the high magical power narrated in the legends</rdfs:comment>
        <rdfs:comment xml:lang="it">Oggetto sacro diventato simbolo dell&apos;impero per via dell&apos;elevata potenza magica narrata nelle leggende</rdfs:comment>
    </Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Inugami -->

