     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:japanesemythologyontology="http://www.modsem.org/ontologies/japanesemythologyontology#"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <Ontology rdf:about="http://www.modsem.org/ontologies/japanesemythologyontology">
        <rdfs:label rdf:datatype="&xsd;string">JapaneseMythologyOntology</rdfs:label>
        <dc:date rdf:datatype="&xsd;string">2024-01-25</dc:date>
        <dc:creator rdf:datatype="&xsd;string">Saverio Accurso</dc:creator>
        <vann:preferredNamespaceUri rdf:datatype="&xsd;anyURI">http://www.modsem.org/ontologies/japanesemythologyontology</vann:preferredNamespaceUri>
        <dc:title xml:lang="en">Japanese Mythology Ontology</dc:title>
        <dc:title xml:lang="it">Ontologia della Mitologia Giapponese</dc:title>
        <vann:preferredNamespacePrefix>jmo</vann:preferredNamespacePrefix>
        <dc:description xml:lang="en">Ontology to describe the main features of Japanese mythology</dc:description>
        <dc:description xml:lang="it">Ontologia per descrivere le principali caratteristiche della mitologia giapponese</dc:description>
        <imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl"/>
        <imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/place.owl"/>
    </Ontology>
    


