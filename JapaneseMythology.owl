    <AnnotationProperty rdf:about="&vann;preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <AnnotationProperty rdf:about="&vann;preferredNamespaceUri"/>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#isRuleEnabled -->

    <AnnotationProperty rdf:about="&japanesemythologyontology;isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Avviene_In -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Avviene_In">
        <rdfs:label xml:lang="it">Avviene_In</rdfs:label>
        <rdfs:comment xml:lang="en">Connect the ritual to the sacred place</rdfs:comment>
        <rdfs:comment xml:lang="it">Collega il rito al luogo sacro</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;Ritual"/>
        <rdfs:range rdf:resource="&japanesemythologyontology;SacredPlace"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Contiene -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Contiene">
        <rdfs:label xml:lang="it">Contiene</rdfs:label>
        <rdfs:comment xml:lang="en">What sacred places does the place contain</rdfs:comment>
        <rdfs:comment xml:lang="it">Quali luoghi sacri contiene il luogo</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;SacredPlace"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/place.owl#isLocationOf"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Ha_Creatura -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Ha_Creatura">
        <rdfs:label xml:lang="it">Ha_Creatura</rdfs:label>
        <rdfs:comment xml:lang="en">The event contains mythological creatures</rdfs:comment>
        <rdfs:comment xml:lang="it">L&apos;evento contiene creature mitologiche</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;Creatura_Mitologica"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Ha_Evento -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Ha_Evento">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="it">Ha_Evento</rdfs:label>
        <rdfs:comment xml:lang="en">The era has the mythological event within it</rdfs:comment>
        <rdfs:comment xml:lang="it">L&apos;epoca ha all&apos;interno l&apos;evento mitologico</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;Epoca"/>
        <rdfs:range rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <inverseOf rdf:resource="&japanesemythologyontology;Parte_Di_Epoca"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Ha_Umano_Leggendario -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Ha_Umano_Leggendario">
        <rdfs:label xml:lang="it">Ha_Umano_Leggendario</rdfs:label>
        <rdfs:comment xml:lang="en">The event contains legendary humans</rdfs:comment>
        <rdfs:comment xml:lang="it">L&apos;evento contiene umani leggendari</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;LegendaryHuman"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#NarrataInEvento -->

    <ObjectProperty rdf:about="&japanesemythologyontology;NarrataInEvento">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="it">Narrata_In</rdfs:label>
        <rdfs:comment xml:lang="en">Which sword is narrated in a mythological event</rdfs:comment>
        <rdfs:comment xml:lang="it">Quale spada viene narrata in un evento mitologico</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;Spada"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Oggetto_In_Luogo_Sacro -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Oggetto_In_Luogo_Sacro">
        <rdfs:label xml:lang="it">Oggetto_In_Luogo_Sacro</rdfs:label>
        <rdfs:comment xml:lang="en">Connect the sacred object to the sacred place</rdfs:comment>
        <rdfs:comment xml:lang="it">Collega l&apos;oggetto sacro al luogo sacro</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredObject"/>
        <rdfs:range rdf:resource="&japanesemythologyontology;SacredPlace"/>
        <propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&japanesemythologyontology;UsatoInRito"/>
            <rdf:Description rdf:about="&japanesemythologyontology;Avviene_In"/>
        </propertyChainAxiom>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Parte_Di_Epoca -->

    <ObjectProperty rdf:about="&japanesemythologyontology;Parte_Di_Epoca">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="it">Parte_Di_Epoca</rdfs:label>
        <rdfs:comment xml:lang="en">The mythological event is part of the era</rdfs:comment>
        <rdfs:comment xml:lang="it">L&apos;evento mitologico è parte dell&apos;epoca</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;Epoca"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#UsatoInRito -->

    <ObjectProperty rdf:about="&japanesemythologyontology;UsatoInRito">
        <rdfs:label xml:lang="it">Usato_In</rdfs:label>
        <rdfs:comment xml:lang="en">Which object is used in a ritual</rdfs:comment>
        <rdfs:comment xml:lang="it">Quale oggetto è usato in un rito</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;Ritual"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredObject"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#comprende -->

    <ObjectProperty rdf:about="&japanesemythologyontology;comprende">
        <rdfs:label xml:lang="it">Comprende</rdfs:label>
        <rdfs:comment xml:lang="en">The sacred place includes deities</rdfs:comment>
        <rdfs:comment xml:lang="it">Il luogo sacro comprende delle divinità</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;Divinità"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredPlace"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#èAssociataA -->

    <ObjectProperty rdf:about="&japanesemythologyontology;èAssociataA">
        <rdfs:label xml:lang="it">è_Associata_A</rdfs:label>
        <rdfs:comment xml:lang="en">Associate the deity with the respective sacred place</rdfs:comment>
        <rdfs:comment xml:lang="it">Associa la divinità al rispettivo luogo sacro</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;Divinità"/>
        <rdfs:range rdf:resource="&japanesemythologyontology;SacredPlace"/>
        <inverseOf rdf:resource="&japanesemythologyontology;comprende"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#è_inserito_in -->

    <ObjectProperty rdf:about="&japanesemythologyontology;è_inserito_in">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="it">è_Inserito_In</rdfs:label>
        <rdfs:comment xml:lang="en">The sacred place is located in the context of a mythological event</rdfs:comment>
        <rdfs:comment xml:lang="it">Il luogo sacro si trova nel contesto di un evento mitologico</rdfs:comment>
        <rdfs:range rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredPlace"/>
    </ObjectProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#è_situato_In -->

    <ObjectProperty rdf:about="&japanesemythologyontology;è_situato_In">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="it">è_Situato_In</rdfs:label>
        <rdfs:comment xml:lang="en">Where the sacred place is located</rdfs:comment>
        <rdfs:comment xml:lang="it">In quale luogo è situato il luogo sacro</rdfs:comment>
        <inverseOf rdf:resource="&japanesemythologyontology;Contiene"/>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredPlace"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/place.owl#hasLocation"/>
    </ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart">
        <rdfs:comment xml:lang="it">Una relazione transitiva che esprime la partenità tra qualsiasi entità, ad es. &quot;il corpo umano ha come parte un cervello&quot;. Quando si specializza questo modello, fare attenzione a limitare il dominio e l&apos;intervallo in modo appropriato, poiché potrebbe essere controintuitivo utilizzare questa relazione arbitrariamente, ad es. tra animali e pianeti. Per una parte intransitiva del modello, vedere &apos;componente&apos;.</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf">
        <rdfs:comment xml:lang="it">Una relazione transitiva che esprime la partenità tra qualsiasi entità, ad es. &quot;il cervello è una parte del corpo umano&quot;. Quando si specializza questo modello, fare attenzione a limitare il dominio e l&apos;intervallo in modo appropriato, poiché potrebbe essere controintuitivo utilizzare questa relazione arbitrariamente, ad es. tra animali e pianeti. Per una parte intransitiva del modello, vedere &apos;componente&apos;.</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/place.owl#hasLocation -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/place.owl#hasLocation">
        <rdfs:comment xml:lang="it">Una localizzazione generica e relativa, che intercorre tra qualsiasi entità. Per esempio. &apos;il gatto è sul tappeto&apos;, &apos;Omar è a Samarcanda&apos;, &apos;la ferita è vicino all&apos;arteria femorale&apos;.
Per le posizioni &quot;assolute&quot;, vedere SpaceRegion</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/place.owl#isLocationOf -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/place.owl#isLocationOf">
        <rdfs:comment xml:lang="it">Una localizzazione generica e relativa, che intercorre tra qualsiasi entità. Per esempio. &quot;Roma è la sede del Papa&quot;, &quot;il fegato è la sede del tumore&quot;.
Essendo una nozione molto generale di localizzazione, viene assunta come transitiva.
Per le posizioni &quot;assolute&quot;, vedere SpaceRegion</rdfs:comment>
    </rdf:Description>
    


