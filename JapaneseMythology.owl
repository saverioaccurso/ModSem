    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#AnnoDiCreazione -->

    <DatatypeProperty rdf:about="&japanesemythologyontology;AnnoDiCreazione">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="it">Anno_di_creazione</rdfs:label>
        <rdfs:comment xml:lang="en">Year in which the sacred object was created</rdfs:comment>
        <rdfs:comment xml:lang="it">Anno in cui è stato creato l&apos;oggetto sacro</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredObject"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </DatatypeProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#AnnoEvento -->

    <DatatypeProperty rdf:about="&japanesemythologyontology;AnnoEvento">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="it">Anno_Evento</rdfs:label>
        <rdfs:comment xml:lang="en">Year in which the mythological event occurred</rdfs:comment>
        <rdfs:comment xml:lang="it">Anno in cui è accaduto l&apos;evento mitologico</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;MythicalEvent"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </DatatypeProperty>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#PotenzaMagica -->

    <DatatypeProperty rdf:about="&japanesemythologyontology;PotenzaMagica">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="it">Potenza_Magica</rdfs:label>
        <rdfs:comment xml:lang="en">Magical power of the sacred object</rdfs:comment>
        <rdfs:comment xml:lang="it">Potenza Magica dell&apos;oggetto sacro</rdfs:comment>
        <rdfs:domain rdf:resource="&japanesemythologyontology;SacredObject"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </DatatypeProperty>
    


