    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Creatura_Mitologica -->

    <rdf:Description rdf:about="&japanesemythologyontology;Creatura_Mitologica">
        <rdfs:label xml:lang="it">CreaturaMitologica</rdfs:label>
        <rdfs:comment xml:lang="it"> creatura leggendaria o &apos;altra entità mitologica</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Divinità -->

    <owl:Class rdf:about="&japanesemythologyontology;Divinità">
        <rdfs:label xml:lang="it">Divinità</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&japanesemythologyontology;èAssociataA"/>
                <owl:onClass rdf:resource="&japanesemythologyontology;SacredPlace"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">Rappresenta l&apos;entità divina nella mitologia giapponese</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.modsem.org/ontologies/japanesemythologyontology#Epoca -->

    <owl:Class rdf:about="&japanesemythologyontology;Epoca">
        <rdfs:label xml:lang="it">Epoca</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&japanesemythologyontology;Ha_Evento"/>
                <owl:onClass rdf:resource="&japanesemythologyontology;MythicalEvent"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">Comprende cicli mitologici o epoche specifiche nella mitologia giapponese</rdfs:comment>
